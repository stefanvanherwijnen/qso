{
  "name": "@qso/app",
  "version": "0.9.4",
  "author": "Stefan van Herwijnen",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc && pnpm run generate:import-map",
    "generate:import-map": "node ./dist/write-import-map.js"
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js",
    "./build": "./dist/node/build.js",
    "./dev": "./dist/node/dev.js",
    "./help": "./dist/node/help.js",
    "./helpers/*": "./dist/helpers/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/types/index.d.ts"
      ],
      "helpers/*": [
        "./dist/types/helpers/*.d.ts"
      ],
      "build": [
        "./dist/types/node/build.d.ts"
      ],
      "dev": [
        "./dist/types/node/dev.d.ts"
      ],
      "help": [
        "./dist/types/node/help.d.ts"
      ]
    }
  },
  "bin": {
    "qso": "./dist/node/cli.js"
  },
  "dependencies": {
    "@quasar/extras": "^1.12.4",
    "@vitejs/plugin-vue": "^2.0.1",
    "@vue/compiler-sfc": "^3.2.29",
    "@vue/server-renderer": "^3.2.29",
    "builtin-modules": "^3.2.0",
    "cac": "^6.7.12",
    "chalk": "^5.0.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.17.2",
    "fastify": "^3.27.0",
    "fastify-sensible": "^3.1.2",
    "fastify-static": "^4.5.0",
    "glob": "^7.2.0",
    "import-meta-resolve": "^1.1.1",
    "magic-string": "^0.25.7",
    "merge-deep": "^3.0.3",
    "middie": "^6.0.0",
    "readline": "^1.3.0",
    "sass": "1.32.13",
    "unplugin-vue-components": "^0.17.13",
    "vite": "^2.8.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/glob": "^7.2.0",
    "@types/merge-deep": "^3.0.0",
    "@types/minimist": "^1.2.2",
    "quasar": "^2.5.4",
    "rollup": "^2.67.1",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "vue-router": "^4.0.12"
  },
  "peerDependencies": {
    "vue": "^3.2.23",
    "vue-router": "^4.0.12"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "index.html",
    "ssr",
    "csr",
    "main.ts",
    "quasar-extras.ts",
    "quasar-wrappers.ts",
    "src/helpers/ssr.ts",
    "!dist/**/*.test.js",
    "!dist/**/test.js"
  ]
}
